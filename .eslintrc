{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/sort-comp": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "comma-dangle": [2, "never"],
    "no-shadow": 0,
    "consistent-return": 0
  },
  "plugins": [
    "react"
  ]
}
